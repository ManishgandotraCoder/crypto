    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-bearer-token": "^2.4.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-input-validator": "^4.5.1",
    "passport": "^0.7.0",

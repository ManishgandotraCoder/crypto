{
  "name": "node_in_ts_structure",
  "version": "1.0.0",
  "description": "Node JS in Typescript",
  "main": "src/app.ts",
  "scripts": {
    "start": "nodemon",
    "build": "rimraf ./build && tsc",
    "test": "mocha test/**.ts",
    "coverage": "nyc ",
    "lint": "eslint .",
    "html": "nyc report --reporter=html",
